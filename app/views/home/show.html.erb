          <div class="mb-8 overflow-hidden rounded-lg">
            <% if @product_url.product_images.attached? %>
              <% @product_url.product_images.each_with_index do |image, index| %>
                <% if index.zero? %>
                  <%= image_tag(rails_blob_url(image), class: 'w-full', "x-show": "product#{index + 1}") %>
                <% else %>
                  <%= image_tag(rails_blob_url(image), class: 'w-full', "x-show": "product#{index + 1}", style: "display: none;") %>
                <% end %>
              <% end %>
            <% end %>
          </div>
          <div class="-mx-4 flex items-center justify-between">
            <% if @product_url.product_images.attached? %>
