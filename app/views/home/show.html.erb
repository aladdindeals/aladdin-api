                <div class="w-1/3 px-4">
                  <button @click="productTwo = true; productOne = false; productThree = false " :class=" productTwo &amp;&amp; 'opacity-60' " class="w-full overflow-hidden rounded-lg">
                    <%= image_tag(rails_blob_url(image), class: 'w-full', alt: "thumbnail-01") %>
                  </button>
                </div>
